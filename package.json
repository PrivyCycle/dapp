{
  "name": "consentcycle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@consentcycle/web",
    "build": "npm run build --workspace=@consentcycle/web",
    "lint": "npm run lint --workspace=@consentcycle/web",
    "preview": "npm run preview --workspace=@consentcycle/web",
    "dev:web": "npm run dev --workspace=@consentcycle/web",
    "build:web": "npm run build --workspace=@consentcycle/web",
    "lint:web": "npm run lint --workspace=@consentcycle/web",
    "preview:web": "npm run preview --workspace=@consentcycle/web",
    "build:contracts": "npm run build --workspace=@consentcycle/contracts",
    "test:contracts": "npm run test --workspace=@consentcycle/contracts",
    "clean:contracts": "npm run clean --workspace=@consentcycle/contracts"
  },
  "devDependencies": {
    "typescript": "~5.8.3"
  }
} 